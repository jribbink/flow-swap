{
  "name": "flow-swap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm run dev:standalone' 'npm run emulator'",
    "dev:standalone": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "emulator": "ts-node ./scripts/start-emulator",
    "deploy:project": "flow project deploy --network=emulator",
    "dev-wallet": "npm run --prefix ../fcl-dev-wallet/ dev",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@onflow/fcl": "^1.0.2",
    "@onflow/types": "^0.0.6",
    "bootstrap": "^5.1.3",
    "elliptic": "^6.5.4",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-dom": "18.1.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "concurrently": "^7.1.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "typescript": "4.6.4"
  },
  "type": "module"
}
